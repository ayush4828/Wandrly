
<% layout("/layouts/boilerplate.ejs") %>
<body>

    <h1>All Listings</h1>
     <form action="/listings/new">
        <button>Create New Listings</button>
     </form>
<ul>
    <%for(listing of allListings){ %>
        <li>
           <a href="/listings/<%=listing._id%>"><%=listing.title%></a>
            
        </li>
    <%}%>

    </ul>
</body>
